{
  "name": "TabAddict",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A light-weight session saver.",
  "browser_action": {
    "default_icon": "icon.png",
    "dnd": "dnd.html",
    "launch": "launch.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ],
  "options_page": "options.html",
  "homepage_url": "http://peterthenelson.com/tabaddict/"
}